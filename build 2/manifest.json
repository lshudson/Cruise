{
  "name": "Cruise",
  "description": "Writing productivity app",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  "icons": {
      "16": "main_logo.png",
      "48": "main_logo.png",
      "128": "main_logo.png"
  },
  "permissions": [
    "notifications", "tabs", "storage", "activeTab", "scripting"
  ], 
  "host_permissions": 
  ["<all_urls>"],
  "background": {
    "service_worker": "sw.js"
  }
}
